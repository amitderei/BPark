<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainController">

    <Label layoutX="290.0" layoutY="50.0" text="Welcome to BPARK">
        <font>
            <Font size="26.0" />
        </font>
    </Label>

    <Button fx:id="loginBtn" layoutX="320.0" layoutY="150.0" onAction="#handleLogin" prefWidth="160.0" text="Login" />

    <Button fx:id="guestBtn" layoutX="320.0" layoutY="220.0" onAction="#handleGoToBack" prefWidth="160.0" text="Enter as Guest" />

</AnchorPane>
